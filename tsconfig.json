{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "strictNullChecks": true
  },
  "include": [
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/**/*.js"
  ],
  "files": [
    "src/index.js"
  ]
}
